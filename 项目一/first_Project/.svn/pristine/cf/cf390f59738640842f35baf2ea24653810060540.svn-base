/*注册页面css样式*//**/
*{
    margin:0;
    padding:0;
}
body{
    background:#999999;
}
#box{
    width:100%;
    margin: 0 auto;
}
#regFrom{
    width:100%;
    display:inline-block;
    color:#ccc;
}
.regTable{
    width:565px;
    margin:0 auto;
    border-spacing:10px;
    position:relative;
    padding-bottom:40px;
    /*background:green;*/
    color:#ffffff;
}
.logo td{
    height:160px;
}
.logo a img{
    position:relative;
    left:-33px;
    margin:0 0 -17px 130px;
}

.call_number  {
    height: 50px;
}
.phone_Number span{
    width:50px;
    line-height:50px;
    font-size:14px;
    color:#fff;
    opacity:0.9;
    padding-right:20px;
}
.phone_Number input{
    width:270px;
    height:50px;
    line-height:50px;
    padding-left:20px;
    color: #ffffff;
    opacity:0.99;
    font-size:14px;
    display:inline-block;
    border:0;
    outline:none;
    background:#cbcbcb;
}
.auth_code  {
    height: 50px;
}
.auth_Number span{
    width:50px;
    line-height:50px;
    font-size:14px;
    color:#fff;
    opacity:0.9;
    padding-right:20px;
}
.auth_Number input{
    width:195px;
    height:50px;
    line-height:50px;
    padding-left:20px;
    color:#FFF;
    opacity:0.99;
    font-size:14px;
    display:inline-block;
    border:0;
    outline:none;
    background:#cbcbcb;
}
.auth_Number  img{
    position:relative;
    top:10px;
    right:0;
}
.auth_Number a{
    text-decoration: none;
    color:#ffffff;
    font-size:12px;
}
.placeholder  {
    height: 50px;
}
.placeholder_Number{
    float:left;
}
.placeholder_Number span{
    width:50px;
    line-height:50px;
    font-size:14px;
    color:#fff;
    opacity:0.9;
    padding-right: 6px;
}
.placeholder_Number input{
    width:270px;
    height:50px;
    line-height:50px;
    padding-left:20px;
    color:#FFF;
    opacity:0.99;
    font-size:14px;
    display:inline-block;
    border:0;
    outline:none;
    background:#cbcbcb;
}
.case{
    width: 127px;
    height: 12px;
    border:1px solid #000;
    background:#F5F5F5;
}
.strngth{
    width: 127px;
    height: 16px;
    color:#4c4c4c;
    margin-top:10px;

}
.strngth span{
    font-size:12px;
    display:inline-block;
    width:41px;
    text-align: center;
    float:left;

}
.degree{
    width:127px;
    float:left;
    margin:10px 0 0 10px;

}

.placeholder_Ok  {
    height: 50px;
}
.placeholder_Ok span{
    width:50px;
    line-height:50px;
    font-size:14px;
    color:#fff;
    opacity:0.9;
    padding-right:6px;
}
.placeholder_Ok input{
    width:270px;
    height:50px;
    line-height:50px;
    padding-left:20px;
    color: #ffffff;
    opacity:0.99;
    font-size:14px;
    display:inline-block;
    border:0;
    outline:none;
    background:#cbcbcb;
}
.agree{
    height: 50px;
}
.agree td{
    position:relative;
    left:-4px;
    top:3px;
    padding-left:75px;
}
.agree span{
    padding-left:6px;
    color:#ffffff;
}
.agree a{
    color:#0c79cc;
    text-decoration: none;
}
.reg td input{
    width: 300px;
    height: 50px;
    border:0;
    outline:none;
    border-radius:4px;
    background:#25292e;
    font-size:18px;
    color:#fff;
    /*margin-top:9px;*/
    cursor: pointer;
    margin-left: 70px;;
}
.bottom td{
    padding:20px 177px 0 75px;
    width:300px;
}
.qq{
    background:url(../img/qqmouse.png) no-repeat;
    width: 36px;
    height: 36px;
    display: inline-block;
    text-decoration: none;
    line-height:36px;
}
.qq:hover{
    opacity:0.5;
}
.sina{
    background:url(../img/xinlangnormal.png) no-repeat;
    width: 36px;
    height: 36px;
    display: inline-block;
    text-decoration: none;
}
.sina:hover{
    opacity:0.5;
}
.douban {
    background: url(../img/doubannormal.png) no-repeat;
    width: 36px;
    height: 36px;
    display: inline-block;
    text-decoration: none;
}
.douban:hover{
    opacity:0.5;
}
.qqwb{
    background: url(../img/tengxunnormal.png) no-repeat;
    width: 36px;
    height: 36px;
    display: inline-block;
    text-decoration: none;
}
.qqwb:hover{
    opacity:0.5;
}
.Right a{
    color:#fff;
    font-size:15px;
    padding-bottom:5px;
    border-bottom:1px solid #fff;
    text-decoration: none;
    font-weight:600;
    margin-left:56px;
    float:right;
}
